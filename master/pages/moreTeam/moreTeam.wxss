.serch {
  width: 700rpx;
  height: 74rpx;
  display: flex;
  align-items: center;
  margin: 0 auto;
  background-color: #f4f4f4;
  border-radius: 4rpx;
  margin-top: 25rpx;
}

.serch input {
  height: 74rpx;
  width: 610rpx;
  font-size: 26rpx;
  margin-left: 17rpx;
}

.serch_img {
  width: 50rpx;
  height: 50rpx;
  margin-left: 8rpx;
}

.serch_img image {
  width: 100%;
  height: 100%;
  display: block;
}

/*团队列表*/
.team_box {
  width: 700rpx;
  margin: 0 auto;
  padding-bottom : 30rpx;
  border-bottom : 2rpx solid #ddd;
}
.team_list {
  display: flex;
  margin-top:30rpx;
  /* padding : 0 30rpx; */
}
.team_left {
  /* flex: 2; */
  width: 250rpx;
  height :250rpx;
  /* width: 200rpx;
  height : 200rpx; */
  border-radius: 10rpx;
  overflow: hidden;
}
.team_left image {
  width: 100%;
  height : 100%;
  object-fit: cover;
  object-position: center center;
}
.team_right {
  margin-left: 20rpx;
  font-size : 34rpx;
  /* flex: 3; */
  width: 400rpx;
  color: #333;
  letter-spacing: 2rpx;
  /* width: 300rpx; */
}
.team_box .team_title {
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.team_right .team_details {
  margin-top:15rpx;
  text-align: justify;
  font-size : 28rpx;
  color : #999;
  /* overflow : hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 5;
  -webkit-box-orient: vertical; */
}

/* 优秀作品 */
.team_g_list_box {
  display: flex;
  /* padding : 0 10rpx; */
  justify-content: space-between;
  margin-top : 20rpx;
  width: 100%;
}
.team_g_list {
  font-size:34rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  padding : 0 20rpx;
}
.team_box_title {
  margin-top: 20rpx;
  font-size : 34rpx;
}
.team_g_name {
  margin:10rpx 0;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.team_g_top {
  width: 320rpx;
  height : 320rpx;
  overflow: hidden;
  border-radius: 10rpx;
}
.team_g_bottom {
  width: 320rpx;
}
.team_g_top image {
  width: 100%;
  height :100%;
  object-fit: cover;
  object-position: center center;
}
.team_g_price .discount {
  color : #cc333b;
}
.team_g_price .initial {
  display: inline-block;
  color : #999;
  transform: scale(0.8);
  transform-origin: center center;
}
.team_box_g swiper {
  height : 475rpx;
}
.team_box_g swiper-item {
  display: flex;
  justify-content: space-between;
  /* padding:0 30rpx 0 0; */
  box-sizing: border-box;
}
.team_box_g swiper-item:last-child {
  padding:0 !important;
}
.team_g_name {
  font-size: 30rpx;
}
.team_g_price {
  font-size: 28rpx;
}

/*商品列表*/

.goods_list {
  margin: 40rpx 15rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}

.list {
  width: 320rpx;
  border-radius: 10rpx;
  overflow: hidden;
  box-shadow: 0 0 20rpx -5rpx #ddd;
  margin-bottom : 30rpx;
  box-sizing: border-box;
  break-inside: avoid;
  padding : 10px;
}
/* .list:first-of-type{
  margin-bottom: 40rpx;
} */

.list_img {
  /* width: 100%; */
  /* width: 325rpx; */
  height: 325rpx;

}

.list_img image {
  width: 100%;
  height: 100%;
  object-fit: cover;
  object-position: center;
  display: block;
}

.list_title {
  width: 100%;
  box-sizing: border-box;
  font-size: 24rpx;
  color: #555;
  margin: 20rpx 0;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}

.list_money {
  width: 100%;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.list_left {
  font-size: 28rpx;
  color: #a5937f;
}

.list_right {
  font-size: 22rpx;
  color: #aaa;
  text-decoration: line-through;
}
.goodsName {
  padding: 0rpx 0;
  margin-top: 40rpx;
  /* border-bottom: 2rpx solid #222; */
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.goods_left {
  width: 404rpx;
  /* height: 34rpx; */
  font-size:24rpx;
  padding : 0rpx 20rpx;
  border-left : 4rpx solid #222222;
  font-weight: bold;
}

.goods_left image {
  width: 100%;
  height: 100%;
  
  display: block;
}