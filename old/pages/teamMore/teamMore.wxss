.serch {
  width: 700rpx;
  height: 74rpx;
  display: flex;
  align-items: center;
  margin: 0 auto;
  background-color: #f4f4f4;
  border-radius: 4rpx;
  margin-top: 25rpx;
}

.serch input {
  height: 74rpx;
  width: 610rpx;
  font-size: 26rpx;
  margin-left: 17rpx;
}

.serch_img {
  width: 50rpx;
  height: 50rpx;
  margin-left: 8rpx;
}

.serch_img image {
  width: 100%;
  height: 100%;
  display: block;
}

/*团队列表*/
.team_box {
  width: 100%;
  box-sizing: border-box;
  padding : 0 40rpx 30rpx; 
  margin: 0 auto;
}
.team_box:last-child .team_box_g{
  border:none;
}
.team_list {
  display: flex;
  margin-top:30rpx;
  /* padding : 0 30rpx; */
}
.team_left {
  /* flex: 2; */
  width: 250rpx;
  height :250rpx;
  /* width: 200rpx;
  height : 200rpx; */
  border-radius: 10rpx;
  overflow: hidden;
}
.team_left image {
  width: 100%;
  height : 100%;
  object-fit: cover;
  object-position: center center;
}
.team_right {
  margin-left: 20rpx;
  font-size : 34rpx;
  /* flex: 3; */
  width: 400rpx;
  color: #333;
  letter-spacing: 2rpx;
  /* width: 300rpx; */
}
.team_box .team_title {
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.team_right .team_details {
  margin-top:15rpx;
  text-align: justify;
  font-size : 28rpx;
  color : #999;
  /* overflow : hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 5;
  -webkit-box-orient: vertical; */
}

/* 优秀作品 */
.team_g_list_box {
  display: flex;
  /* padding : 0 10rpx; */
  justify-content: space-between;
  margin-top : 20rpx;
  width: 100%;
}
.team_g_list {
  font-size:34rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  padding : 0 20rpx;
}
.team_box_title {
  margin-top: 20rpx;
  font-size : 34rpx;
}
.team_g_name {
  margin:10rpx 0;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.team_g_top {
  width: 320rpx;
  height : 320rpx;
  overflow: hidden;
  border-radius: 10rpx;
}
.team_g_bottom {
  width: 320rpx;
}
.team_g_top image {
  width: 100%;
  height :100%;
  object-fit: cover;
  object-position: center center;
}
.team_g_price .discount {
  color : #cc333b;
}
.team_g_price .initial {
  display: inline-block;
  color : #999;
  transform: scale(0.8);
  transform-origin: center center;
}
.team_box_g {
  padding-bottom:32rpx;
  border-bottom: 4rpx solid #ddd;
}
.team_box_g swiper {
  height : 475rpx;
}
.team_box_g swiper-item {
  display: flex;
  justify-content: space-between;
  /* padding:0 30rpx 0 0; */
  box-sizing: border-box;
}
.team_box_g swiper-item:last-child {
  padding:0 !important;
}
.team_g_name {
  font-size: 30rpx;
}
.team_g_price {
  font-size: 28rpx;
}
.team_imgbox{
  height: 255prx;
  background-color: #EEEEEE;
  border-radius: 10rpx;
  margin-top: 22rpx;
  display: flex;
  align-items: center;
  padding: 25rpx;
  position: relative;
  justify-content: center;
}

.imgbox{
  width: 170rpx;
  height: 170rpx;
  display: inline-block;
  background-color: #fff;
  border-radius: 10rpx;
  margin-right:15rpx; 
}
.imgbox image{
  width: 100%;
  height: 100%;
  display: block;
  border-radius: 10rpx;
}
.next{
  width: 30rpx;
  height: 30rpx;
  margin-left: 18rpx;
  position: absolute;
  right:25rpx;
  top:100rpx;
}
.next image{
  width: 100%;
  height: 100%;
  display: block;
}
.prev{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  left:25rpx;
  top:100rpx;
  transform: rotate(180deg)
}
.prev image{
  width: 100%;
  height: 100%;
  display: block;
}


.no_goods {
  font-size : 26rpx;
  color : #999;
  position: absolute;
  left : 50%;
  transform: translateX(-50%);
}
.price_bottom {
  font-size:26rpx;
  color : #555;
  margin-top:17rpx;
  margin-bottom:25rpx;
}