}
.r_select {
  width: 45rpx;
  height:45rpx;
  position: relative;
}
.r_select image {
  width: 100%;
  height:100%;
  object-fit: cover;
  object-position: center center;
  position: absolute;
}
.z_index {
  z-index: 999;
}

.finish {
  display: flex;
  justify-content: center;
}
.finish text {
  margin-top: 40rpx;
  padding : 10rpx 40rpx;
  border-radius: 10rpx;
  background: #ab9a88;
  color:#fff;
}
.dialog {
  width: 100%;
  height : 100%;
  position: fixed;
  z-index: 2000;
  background: rgba(0,0,0,0.5);
}

.icon_select {
  animation: icon_select 0.3s linear;
}
.icon_no_select {
  animation: none;
}
@keyframes icon_select {
  0% {
    opacity: 0;
    transform: rotate(270deg) scale(0.5);
  }
  100% {
    opacity: 1;
    transform: rotate(0) scale(1);
  }
}

.show_modal {
  animation: show_modal 0.5s linear;
}
@keyframes show_modal {
  0% {
    opacity: 0;
    transform: translateY(100rpx);
  }
  100% {
    opacity: 1;
    transform: translateY(0rpx);
  }
}