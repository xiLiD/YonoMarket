/* 切换动画*/
.animation_box_center {
  position: relative;
}
.animation_box {
  width: 100%;
  position: absolute;
}
.aleft_n1 {
  transform: translateX(-100%);
}
.aleft_n2 {
  transform: translateX(100%);
}
.aleft {
  z-index : 999;
  transform: translateX(0);
  transition: 0.2s linear;
}


/* 弹窗 */
.reason_box {
  position: fixed;
  top:0;
  /* background: rgba(0,0,0,0.5); */
  width: 100%;
  height : 100%;
  z-index : 9999;
}
.reason {
  width: 690rpx;
  margin: 0 auto;
  border-radius: 10rpx;
  height : 315rpx;
  background: #fff;
  position: fixed;
  /* top : 670rpx; */
  /* left: 50%;
  transform: translateX(-50%); */
  z-index: 9999;
  left: 30rpx;
  bottom:30rpx;
  top:initial;
}
.reason_title {
  margin: 40rpx 0 50rpx;
  text-align: center;
  color: #222;
  font-weight: bold;
  font-size:28rpx;
  padding:0 40rpx;
  box-sizing: border-box;
}
.reason_title .reason_btn {
  display: flex;
  justify-content: space-between;
  font-size:28rpx;
  letter-spacing: 2rpx;
}
.cancle {
