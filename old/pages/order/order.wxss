.header_top {
  display: flex;
  justify-content: space-between;
  width: 100%;
  box-sizing: border-box;
  padding : 20rpx 30rpx; 
}
.big_box  {
  margin-top : 0;
}

/* 导航底部动画*/
.tab{
  font-size: 26rpx;
  color: #222222;
  font-weight: bold;
  /* margin: 30rpx 47rpx; */
  /* padding-bottom: 10rpx; */
  position: relative;
  border: none;
  /* margin-top: 28rpx; */
}
.tab .line_bottom{
  position: absolute;
  bottom : 0;
  height : 4rpx;
  width: 0;
  background: #A5937F;
}
.tab.animate_active .line_bottom{
  width: 100%;
  transition: 0.2s linear;
}
.animate_active{
  color: #A5937F;
}
.details {
  color : #999;
  font-size : 26rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 4;
  overflow: hidden;
}

.icon_num {
  right:-35rpx;
  top:0rpx;
}
.animation_box_center  {
  padding-top:100rpx;
}