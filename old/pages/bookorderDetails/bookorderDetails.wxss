.show_modal {
  animation: show_modal 0.5s linear;
}
@keyframes show_modal {
  0% {
    opacity: 0;
    transform: translateY(100rpx);
  }
  100% {
    opacity: 1;
    transform: translateY(0rpx);
  }
}
.no_modal {
