.icon_select {
  animation: icon_select 0.5s linear;
}
.icon_no_select {
  animation: none;
}
@keyframes icon_select {
  0% {
    opacity: 0;
    transform: rotate(270deg) scale(0.5);
  }
  100% {
    opacity: 1;
    transform: rotate(0) scale(1);
  }
}
